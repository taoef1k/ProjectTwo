			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.cxrus.model.impl.ArticlesImpl" name="Articles">
		<table name="AR_Articles" />
		<attributes />
	</entity>
	<entity class="com.cxrus.model.impl.CategoryArticlesImpl" name="CategoryArticles">
		<table name="AR_CategoryArticles" />
		<attributes />
	</entity>
</entity-mappings>